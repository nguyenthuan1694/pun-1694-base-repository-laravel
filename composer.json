{
    "name": "thuan-1694/base-repository-laravel",
    "description": "Base repository implementation for Laravel",
    "type": "project",
    "license": "BSD-2-Clause",
    "authors": [
        {
          "name": "Nguyen Van Thuan",
          "email": "nguyenthuan1694@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.1",
        "illuminate/support": "~5.1",
        "illuminate/database": "~5.1"
      },
      "require-dev": {
        "phpunit/phpunit": "^4.8",
        "mockery/mockery": "^0.9.4"
      },
      "autoload": {
        "psr-4": {
          "Kenini\\": "src/"
        }
      },
      "autoload-dev": {
        "files": [
          "tests/TestFunctions.php"
        ],
        "psr-4": {
          "Kenini\\Test\\": "tests"
        }
      },
      "extra": {
        "laravel": {
          "providers": [
              "Kenini\\RepositoryServiceProvider"
          ]
        }
      },
      "archive" : {
        "exclude": [
          "/tests"
        ]
      }
}
