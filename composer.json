{
    "name": "thuan-1694/base-repository-laravel",
    "description": "Base repository implementation for Laravel",
    "type": "project",
    "license": "BSD-2-Clause",
    "authors": [
        {
          "name": "Nguyen Van Thuan",
          "email": "nguyenthuan1694@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.2",
        "laravel/framework": "^6.1",
        "illuminate/support": "~6.1",
        "illuminate/database": "~6.1"
      },
      "require-dev": {
        "phpunit/phpunit": "^8.0",
        "mockery/mockery": "^1.0"
      },
      "autoload": {
        "psr-4": {
          "Kenini\\": "src/"
        }
      },
      "autoload-dev": {
        "files": [
          "tests/TestFunctions.php"
        ],
        "psr-4": {
          "Kenini\\Test\\": "tests"
        }
      },
      "extra": {
        "laravel": {
          "providers": [
              "Kenini\\RepositoryServiceProvider"
          ]
        }
      },
      "archive" : {
        "exclude": [
          "/tests"
        ]
      }
}
